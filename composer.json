{
    "name": "farzai/geonames",
    "type": "library",
    "description": "This package provides a simple way to download Geonames data and format it for friendly use.",
    "keywords": ["geonames", "countries", "data"],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "ext-zip": "*",
        "farzai/transport": "^1.0.0"
    },
    "require-dev": {
        "pestphp/pest": "^1.20",
        "spatie/ray": "^1.28",
        "mockery/mockery": "^1.4",
        "guzzlehttp/guzzle": "^7.5",
        "laravel/pint": "^1.2",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Farzai\\Geonames\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Farzai\\Geonames\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "parsilver",
            "role": "DevOps"
        }
    ],
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint",
        "analyse": "vendor/bin/phpstan analyse"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
